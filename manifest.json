{
    "manifest_version": 3,
    "name": "Bookmark Manager Pro",
    "version": "1.0",
    "description": "Professional bookmark manager with categories",
    "permissions": [
        "storage",
        "windows",
        "tabs",
        "scripting",
        "activeTab",
        "system.display"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
    "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}